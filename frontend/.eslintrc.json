{
    "extends": [
        "eslint:recommended",
        "react-app",
        "react-app/jest",
        "plugin:react/recommended",
        "plugin:import/recommended",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off"
    },
    "plugins": [
    ],
    "settings" : {
        "import/resolver": {
            "node": {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        }
    }
}
