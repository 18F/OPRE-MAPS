{
    "extends": [
        "eslint:recommended",
        "react-app",
        "react-app/jest",
        "plugin:react/recommended",
        "plugin:react/jsx-runtime",
        "plugin:import/recommended",
        "plugin:prettier/recommended",
        "plugin:cypress/recommended"
    ],
    "rules": {
        "react/prop-types": "off"
    },
    "plugins": [
    ],
    "ignorePatterns": [
        "**/*.css",
        "**/*.svg",
        "**/*.png"
    ],
    "settings" : {
        "import/resolver": {
            "node": {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        }
    }
}
