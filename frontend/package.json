{
  "name": "opre-ops",
  "version": "1.0.0",
  "license": "CC0-1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "1.8.5",
    "@uswds/uswds": "3.1.0",
    "axios": "0.27.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.2",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "react-select": "5.4.0"
  },
  "devDependencies": {
    "axe-core": "4.4.3",
    "cypress": "10.7.0",
    "cypress-axe": "1.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.7.1"
  },
  "scripts": {
    "start": "DISABLE_ESLINT_PLUGIN=true REACT_APP_BACKEND_DOMAIN=http://localhost:8080 react-scripts start",
    "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "test:e2e:interactive": "REACT_APP_BACKEND_DOMAIN=http://localhost:8080 cypress open --config-file ./cypress.config.js",
    "test:e2e": "REACT_APP_BACKEND_DOMAIN=http://localhost:8080 cypress run --config-file ./cypress.config.js --headless",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**'"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "lines": 90
      }
    },
    "coveragePathIgnorePatterns": [
      ".jsx$",
      "src/applicationContext/.+",
      "src/helpers/backend.js$",
      "src/reportWebVitals.js$"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
