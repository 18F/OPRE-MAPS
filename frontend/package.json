{
  "name": "opre-ops",
  "version": "1.0.0",
  "license": "CC0-1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "1.8.4",
    "axios": "0.27.2",
    "puppeteer-core": "^16.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.2",
    "react-router-dom": "6.3.0",
    "react-select": "5.4.0",
    "web-vitals": "2.1.4"
  },
  "devDependencies": {
    "cypress": "10.5.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "prettier": "2.7.1",
    "puppeteer": "^16.1.0",
    "puppeteer-firefox": "^0.5.1",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "DISABLE_ESLINT_PLUGIN=true REACT_APP_BACKEND_DOMAIN=http://localhost:8080 react-scripts start",
    "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "test": "react-scripts test --testPathIgnorePatterns=.*e2e.test.js",
    "test:coverage": "react-scripts test --coverage",
    "test:e2e": "REACT_APP_BACKEND_DOMAIN=http://localhost:8080 react-scripts test --testPathPattern=*e2e.test.js",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "lines": 90
      }
    },
    "coveragePathIgnorePatterns": [
      ".jsx$",
      "src/applicationContext/.+",
      "src/helpers/backend.js$",
      "src/reportWebVitals.js$"
    ]
  }
}
