        "lodash": "4.17.21",
        "react": "18.2.0",
        "react-currency-format": "1.1.0",
        "react-dom": "18.2.0",
        "react-markdown": "8.0.7",
        "react-redux": "8.1.1",
        "react-router-dom": "6.14.0",
        "react-select": "5.7.3",
        "sass": "1.63.6",
        "sass-loader": "13.3.2",
        "vest": "4.6.11"
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "14.0.0",
        "@types/jest": "^29.5.2",
        "@types/testing-library__jest-dom": "^5.14.6",
        "@types/testing-library__react": "^10.2.0",
        "@typescript-eslint/eslint-plugin": "^5.61.0",
        "@typescript-eslint/parser": "^5.61.0",
        "@uswds/compile": "1.0.0",
        "@vitejs/plugin-react": "^4.0.1",
        "axe-core": "4.7.2",
        "cypress": "12.16.0",
        "cypress-axe": "1.4.0",
        "cypress-localstorage-commands": "2.2.3",
        "eslint": "^8.44.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-cypress": "2.13.3",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "jose": "4.14.4",
        "msw": "1.2.2",
        "prettier": "^3.0.0",
        "redux-mock-store": "^1.5.4",
        "vite": "^4.3.9",
        "vite-jsconfig-paths": "^2.0.1",
        "vite-plugin-babel": "^1.1.3",
        "vite-plugin-eslint": "^1.8.1",
        "vite-plugin-svgr": "^3.2.0"
    },
    "scripts": {
        "start": "DISABLE_ESLINT_PLUGIN=true vite --host",
        "start:debug": "DISABLE_ESLINT_PLUGIN=true vite --inspect=0.0.0.0:9229 start",
        "build": "DISABLE_ESLINT_PLUGIN=true vite build",
        "test": "vite test",
        "test:coverage": "vite test --coverage",
        "test:e2e:interactive": "cypress open --config-file ./cypress.config.js",
        "test:e2e": "cypress run --config-file ./cypress.config.js --headless",
        "test:e2e:debug": "DEBUG=cypress:* cypress run --config-file ./cypress.config.js --headless",
        "lint": "eslint './src/**' --ext .js,.jsx,.ts,.tsx",
        "cypress:open": "cypress open",
        "uswds:update": "npx gulp compile"
    },
    "jest": {
        "coverageThreshold": {
            "global": {
                "lines": 90
            }
        },
        "coveragePathIgnorePatterns": [
            ".jsx$",
            "src/applicationContext/.+",
            "src/helpers/backend.js$",
            "src/reportWebVitals.js$",
            "src/uswds/"
        ],
        "transformIgnorePatterns": [
            "node_modules/(?!crypto-random-string)/"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
