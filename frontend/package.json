    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "6.5.1",
        "@fortawesome/free-regular-svg-icons": "6.5.1",
        "@fortawesome/free-solid-svg-icons": "6.5.1",
        "@fortawesome/react-fontawesome": "0.2.0",
        "@nivo/bar": "0.84.0",
        "@nivo/core": "0.84.0",
        "@nivo/pie": "0.84.0",
        "@reduxjs/toolkit": "1.9.7",
        "@uswds/uswds": "3.7.0",
        "axios": "1.6.2",
        "babel-plugin-macros": "3.1.0",
        "clsx": "2.0.0",
        "crypto-random-string": "5.0.0",
        "js-cookie": "3.0.5",
        "jwt-decode": "3.1.2",
        "lodash": "4.17.21",
        "node-gyp": "9.4.1",
        "react": "18.2.0",  
        "react-currency-format": "1.1.0",
        "react-dom": "18.2.0",
        "react-markdown": "9.0.1",
        "react-modal": "3.16.1",
        "react-redux": "8.1.3",
        "react-router-dom": "6.20.1",
        "react-scripts": "5.0.1",
        "react-select": "5.8.0",
        "sass": "1.69.5",
        "sass-loader": "13.3.2",
        "vest": "5.2.2"
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "7.21.11",
        "@testing-library/jest-dom": "5.17.0",
        "@testing-library/react": "14.1.2",
        "@tsconfig/create-react-app": "2.0.1",
        "@types/jest": "29.5.10",
        "@types/testing-library__jest-dom": "5.14.9",
        "@types/testing-library__react": "10.2.0",
        "@uswds/compile": "1.1.0",
        "axe-core": "4.8.2",
        "cypress": "13.6.0",
        "cypress-axe": "1.5.0",
        "cypress-localstorage-commands": "2.2.5",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-cypress": "2.15.1",
        "eslint-plugin-import": "2.29.0",
        "eslint-plugin-prettier": "5.0.1",
        "eslint-plugin-react": "7.33.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-react-refresh": "0.4.5",
        "history": "5.3.0",
        "jose": "5.1.3",
        "msw": "1.3.2",
        "prettier": "3.1.0",
        "redux-mock-store": "1.5.4"
    },
    "scripts": {
        "start": "DISABLE_ESLINT_PLUGIN=true react-scripts start",
        "start:debug": "DISABLE_ESLINT_PLUGIN=true react-scripts --inspect=0.0.0.0:9229 start",
        "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
        "test": "react-scripts test",
        "test:coverage": "react-scripts test --coverage",
        "test:e2e:interactive": "cypress open --config-file ./cypress.config.js",
        "test:e2e": "cypress run --config-file ./cypress.config.js --headless",
        "test:e2e:debug": "DEBUG=cypress:* cypress run --config-file ./cypress.config.js --headless",
        "eject": "react-scripts eject",
        "lint": "eslint './src/**'",
        "cypress:open": "cypress open",
        "uswds:update": "bunx gulp compile"
    },
    "jest": {
        "coverageThreshold": {
            "global": {
                "lines": 90
            }
        },
        "coveragePathIgnorePatterns": [
            ".jsx$",
            "src/applicationContext/.+",
            "src/helpers/backend.js$",
            "src/reportWebVitals.js$",
            "src/uswds/"
        ],
        "transformIgnorePatterns": [
            "node_modules/(?!crypto-random-string)/"
        ],
        "moduleNameMapper": {
            "^@uswds/uswds/js$": "<rootDir>/src/uswds/js/uswds-mock.js"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
